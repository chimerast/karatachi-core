<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%-5p %c - %m%ex%n</pattern>
    </encoder>
  </appender>
  <logger name="org.seasar">
    <level value="INFO" />
  </logger>
  <logger name="org.karatachi">
    <level value="INFO" />
  </logger>
  <root>
    <level value="ERROR" />
    <appender-ref ref="STDOUT" />
  </root>
</configuration>
